<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="club.dlblog.sqlite.mapper.ProMapper">    <select id="list" resultType="club.dlblog.sqlite.entity.ProEntity">        SELECT * FROM pro    </select>    <select id="query" parameterType="club.dlblog.sqlite.entity.ProEntity"            resultType="club.dlblog.sqlite.entity.ProEntity">        SELECT * FROM pro        WHERE 1=1        <if test="databaseKey!=null and databaseKey!=''">            AND database_key like CONCAT(CONCAT('%',#{databaseKey}),'%')        </if>    </select>    <insert id="add" parameterType="club.dlblog.sqlite.entity.ProEntity">        INSERT INTO(database_key,database_url)        VALUES(#{databaseKey},#{databaseUrl})    </insert>    <delete id="delAll">        DELETE FROM pro    </delete>    <delete id="del" parameterType="java.lang.Integer">        DELETE FROM pro WHERE id =#{id}    </delete></mapper>